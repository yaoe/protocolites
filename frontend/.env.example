# Environment Variables for Protocolites Frontend
# Copy this file to .env.local and fill in the values

# Ponder Indexer URL for SQL queries
# Local development: http://localhost:42069/sql
# Production: https://protocolites-production.up.railway.app/sql
NEXT_PUBLIC_INDEXER_URL=

# Ethereum RPC URL (optional, for direct contract interaction)
# NEXT_PUBLIC_RPC_URL=

# Contract addresses (will be set when contracts are deployed)
# NEXT_PUBLIC_MASTER_CONTRACT_ADDRESS=
# NEXT_PUBLIC_FACTORY_CONTRACT_ADDRESS=
# NEXT_PUBLIC_RENDERER_CONTRACT_ADDRESS=
